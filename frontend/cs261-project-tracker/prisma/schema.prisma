generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../../../database/test.db"
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model project_tasks {
  id          Int   @id
  project     BigInt?
  name        String?
  description String?
  start_date  DateTime?
  deadline    DateTime?
  end_date    DateTime?
  progress    String?
  risk        Int?

}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model users {
  id               Int @id @default(autoincrement())
  forename         String
  surname          String
  email            String @unique
  password         String
  years_experience Int?

}
